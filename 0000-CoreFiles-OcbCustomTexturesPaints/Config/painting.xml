<configs>

	<append xpath="/paints">

		<opaque id="test_colors" name="txName_test_colors" x="0" y="0" w="2" h="2" blockw="2" blockh="2">
			<!-- Diffuse must not have an alpha channel or texture copy will fail! -->
			<property name="Diffuse" value="512:512:1:1:0,512:512:0:1:1,512:512:1:0:0,512:512:0:0:1" />
			<!-- Specular has the format: Metallic=R, Occlusion=G, Emission=B, Roughness=A -->
			<property name="Specular" value="512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85,512:512:0.1:1:0:0.85" />
			<!-- Although normal maps are missing, we will fill in and use neutral/flat one -->
			<property name="Group" value="txGroupDecoration"/>
			<property name="SwitchUV" value="False" />
			<property name="GlobalUV" value="False" />
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

		<opaque id="test_green" name="txName_test_green" x="0" y="0" w="1" h="1" blockw="1" blockh="1">
			<!-- Diffuse must not have an alpha channel or texture copy will fail! -->
			<property name="Diffuse" value="512:512:0:1:0" />
			<!-- Specular has the format: Metallic=R, Occlusion=G, Emission=B, Roughness=A -->
			<property name="Specular" value="512:512:0.1:1:0:0.85" />
			<!-- Although normal maps are missing, we will fill in and use neutral/flat one -->
			<property name="Group" value="txGroupDecoration"/>
			<property name="SwitchUV" value="False" />
			<property name="GlobalUV" value="False" />
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

		<opaque id="bark_pine_all_faces" name="txName_bark_pine_all_faces" x="0" y="0" w="2" h="2" blockw="2" blockh="2">
			<property name="Diffuse" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_002_diffuse.jpg,assets/bark_pine_001_diffuse.jpg,assets/bark_pine_002_diffuse.jpg,assets/bark_pine_003_diffuse.jpg"/>
			<property name="Normal" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_002_normal.jpg,assets/bark_pine_001_normal.jpg,assets/bark_pine_002_normal.jpg,assets/bark_pine_003_normal.jpg"/>
			<property name="Group" value="txGroupDecoration"/>
			<property name="SwitchUV" value="False" />
			<property name="GlobalUV" value="False" />
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

		<opaque id="bark_pine_001" name="txName_bark_pine_001">
			<property name="Diffuse" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_001_diffuse.jpg"/>
			<property name="Normal" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_001_normal.jpg"/>
			<property name="Group" value="txGroupDecoration"/>
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

		<opaque id="bark_pine_002" name="txName_bark_pine_002" w="1" h="1" blockw="1" blockh="1">
			<property name="Diffuse" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_002_diffuse.jpg"/>
			<property name="Normal" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_002_normal.jpg"/>
			<property name="Group" value="txGroupDecoration"/>
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

		<opaque id="bark_pine_003" name="txName_bark_pine_003" x="0" y="0" w="1" h="1" blockw="1" blockh="1">
			<property name="Diffuse" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_003_diffuse.jpg"/>
			<property name="Normal" value="#@modfolder:Resources/Atlas.unity3d?assets/bark_pine_003_normal.jpg"/>
			<property name="Group" value="txGroupDecoration"/>
			<property name="Color" value="0.4823529,0.3490196,0.2901961"/>
			<property name="Material" value="wood" />
			<property name="PaintCost" value="1" />
		</opaque>

	</append>

</configs>